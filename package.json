{
  "name": "node-test-app",
  "version": "1.0.0",
  "description": "node test app used for testing CD integration",
  "scripts": {
    "start": "node ./bin/server",
    "start:watch": "nodemon ./bin/server",
    "watch": "nodemon ./bind/server",
    "clean:cover": "rimraf ./docs/coverage",
    "cover": "run-s clean:cover cover:js",
    "cover:js": "nyc mocha --recursive ./test/**/*.test.js",
    "lint:js": "standard",
    "test:js": "mocha --recursive ./test/**/*.test.js",
    "test": "npm run test:js"
  },
  "engines": {
    "node": "8.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jlfabi/IT-697/tree/master/code"
  },
  "author": "jfabi",
  "dependencies": {
    "ajv": "^6.10.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "fast-json-stringify": "^1.15.3",
    "http-errors": "^1.7.3",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "lint-staged": "^8.2.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "standard": "^12.0.1",
    "supertest": "^4.0.2"
  },
  "standard": {
    "globals": [
      "before",
      "after",
      "describe",
      "it"
    ]
  }
}
